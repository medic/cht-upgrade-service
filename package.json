  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/medic/cht-upgrade-service/issues"
  },
  "homepage": "https://github.com/medic/cht-upgrade-service#readme",
  "dependencies": {
    "body-parser": "^1.19.2",
    "express": "^4.17.3"
  },
  "devDependencies": {
    "@medic/eslint-config": "^1.1.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.9.0",
    "mocha": "^9.2.1",
    "node-fetch": "^2.6.7",
    "nyc": "^15.1.0",
    "rewire": "^6.0.0",
    "sinon": "^13.0.1"
  }
